{
  "name": "euphony",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.4.4",
    "axios": "^0.18.0",
    "bootstrap": "4.1.3",
    "chokidar-cli": "^1.2.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts-ts": "^3.1.0",
    "reactstrap": "^6.5.0"
  },
  "scripts": {
    "dev": "watch 'yarn build' src",
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/styles/scss/ -o src/styles/scss/",
    "build-task:autoprefixer": "postcss src/styles/scss/*.css --use autoprefixer -d src/styles/scss",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/styles/scss/**/*.scss' -c 'npm run sass:build'",
    "develop": "npm-run-all -p sass:* dev",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test",
    "eject": "react-scripts-ts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/chrome": "^0.0.75",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/reactstrap": "^6.4.3",
    "typescript": "^3.2.1",
    "watch": "^1.0.2"
  }
}
